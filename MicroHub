        Functions.Train("Speed")
    end
}

Player:Button{
	Name = "Heal",
    Description = "Heals You"; -- Description
	Callback = function() 
        Functions.TakeDamage(-100)
    end
}

Player:Button{
	Name = "Heal Players",
    Description = "Heals All Players"; -- Description
	Callback = function() 
        Functions.HealPlayers()
    end
}

